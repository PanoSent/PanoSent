        "dialogue": [
            {
                "index": 0,
                "speaker": 0,
                "utterance": "The floor routine is incredibly dynamic. It captures the audience's attention from start to finish.",
                "annotation": "The <target_01> floor routine </target_01> is <opinion_01> incredibly dynamic </opinion_01>. <rationale_01> It captures the audience's attention from start to finish </rationale_01>.",
                "modality": {
